// 0ms Runtime Beats 100%
// 29.4MB Memory Beats 10.94%
public class Solution {
    public int XorOperation(int n, int start) {
        int[] nums = new int[n];
        int output = 0;
        for (int i = 0; i < n; i++)
        {
            nums[i] = start + 2 * i;
        }

        foreach (int i in nums)
        {
            output ^= i;
        }

        return output;
    }
}
