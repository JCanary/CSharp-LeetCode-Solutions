// 1ms Runtime 89.04%
// 50MB Memory Beats 21.92%
public class Solution {
    public int[] GetSneakyNumbers(int[] nums) {
        int[] output = new int[2];

        int pointer = 0;

        var dict = new Dictionary<int, int> { };

        foreach (int i in nums)
        {
            if (!dict.ContainsKey(i))
            {
                dict.Add(i, 0);
            }
            else
            {
                output[pointer] = i;
                pointer++;
                if (pointer == 2) break;
            }
        }

        return output;
    }
}
