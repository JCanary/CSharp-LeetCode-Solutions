// 0ms Runtime Beats 100%
// 30.84MB Memory Beats 64.22%
public class Solution {
    public bool IsPowerOfFour(int n) {
        long i = 1;

        while (i != n)
        {
            i = i * 4;
            if (i > n)
            {
            return false;
            }
        }

        return true;
    }
}
