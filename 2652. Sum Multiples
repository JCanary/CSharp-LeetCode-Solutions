// 6ms Runtime Beats 78.26%
// 29.36MB Memory Beats 30.43%
public class Solution {
    public int SumOfMultiples(int n) {
        int output = 0;
        for (int i = 1; i <= n; i++)
        {
            if (i % 3 == 0 || i % 5 == 0 || i % 7 == 0) output += i;
        }
        return output;
    }
}
