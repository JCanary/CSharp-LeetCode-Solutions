// 0ms Runtime Beats 100%
// 41.52MB Memory Beats 30.86%
public class Solution {
    public int NumIdenticalPairs(int[] nums) {
        int output = 0;

        for (int i = 0; i < nums.Length - 1; i++)
        {
            for (int j = i + 1; j < nums.Length; j++)
            {
                if (nums[i] == nums[j]) output++;
            }
        }

        return output;
    }
}
